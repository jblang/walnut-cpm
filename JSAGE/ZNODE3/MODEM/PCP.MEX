DSC
KSET PARITY EVEN
STAT WTECHO OFF
DIAL ???-???? #                   <-- Put your local Telenet access # here
WAIT SILENCE 2
SENDOUT "^M"
WAIT STRING 3 "TERMINAL="
SENDOUT "D1"
WAIT STRING 2 "@"
SENDOUT "C DIAL{1}//12,????????"  <-- Put your PC Pursuit User ID here
WAIT STRING 2 "PASSWORD = "
SENDOUT "????????"                <-- Put your PC Pursuit password here
WAIT STRING 3 "CONNECTED"
KSET PARITY NONE
SENDOUT "ATZ"
WAIT STRING 2 "OK"
SENDOUT "^M@"
WAIT STRING 2 "@"
SENDOUT "SET? 1:0"
WAIT STRING 2 "PAR 1:0"
SENDOUT "CONT"
CALL {2} #
