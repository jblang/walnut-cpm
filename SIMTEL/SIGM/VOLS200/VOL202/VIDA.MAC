;
;  VIDSKP - Skip over video string pted to by HL; pt to byte after string
;
vidskp::
	mov	a,m	;get next char
	inx	h	;pt to next
	ora	a	;done if zero
	rz
	cpi	'\'	;literal value?
	jnz	vidskp	;continue if not
	inx	h	;pt to after literal value
	jmp	vidskp

	end
