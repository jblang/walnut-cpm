805 '	print a file
810 ' 1.10
815 IF SKIPFILES%=1 THEN RETURN
820 IF FIL$="NONE" THEN RETURN
825 SETUSERNUMBER%=USERNUMBER%: GOSUB 300
830 NOFILE%=0
835 OPEN "I",2,DRIVE$+FIL$
840 IF NOFILE%<>0 THEN CLOSE #2:
	COMMENT$=DRIVE$+FIL$+" not found": GOSUB 8000: RETURN
845 IF NOCTLK% = 0 THEN 
	A$=CRLF$+"K to skip, S to pause."+CRLF$: GOSUB 400
850 CR%=2
855 IF EOF(2) OR (BREAK% AND NOCTLK%=0) THEN
	 860 ELSE LINE INPUT #2,A$:GOSUB 400:GOTO 855
860 CLOSE #2
865 NOCTLK%=0
870 RETURN
