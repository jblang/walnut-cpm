* DATE 07/09/84  13:21
* Screen print
@ 22,0 SAY 'PRINT OPTIONS: S (SINGLE SPACE), D (DOUBLE SPACE), N (NO PRINTING)       '
STOR 'Y' TO PRNTSCRN
STOR 'N' TO PRNTOPT
@ 22,67 GET PRNTOPT PICTURE '!'
READ
DO WHIL @(PRNTOPT, 'SDN') = 0
 @ 22,67 GET PRNTOPT PICTURE '!'
 READ
ENDD
IF PRNTOPT = 'S'
 STOR 1 TO LINEMULT
ENDI
IF PRNTOPT = 'D'
 STOR 2 TO LINEMULT
ENDI
IF PRNTOPT = 'N'
 STOR 'N' TO PRNTSCRN
ENDI
IF PRNTSCRN = 'Y'
 SET FORMAT TO PRINT
 EJEC
 @ LINEMULT*4,2 SAY 'Address:'
 @ LINEMULT*4,12 SAY BADDR
 @ LINEMULT*4,56 SAY 'Code:'
 @ LINEMULT*4,63 SAY BCODE
 @ LINEMULT*5,5 SAY 'City:'
