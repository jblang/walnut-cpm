;	PROGRAM TO SEND CHARACTERS TO PUNCHER
;	AND RECIEVE FROM READER
;
;	PROGRAM MAY BE CALLED FROM FORTAN AS FOLLOWS
;
;	LOGICAL READER
;	LOGICAL N
;	EXTERNAL READER
;	   .
;	   .
;	   .
;	CALL	PUNCH(N)
;	N=READER
	GLOBAL	PUNCH,READER
BDOS	EQU	0005H
WPUN	EQU	4
RRDR	EQU	3
;
;
READER:	MVI	C,RRDR
	JMP	BDOS
PUNCH:	MVI	C,WPUN
	MOV	E,M
	JMP	BDOS
	END
