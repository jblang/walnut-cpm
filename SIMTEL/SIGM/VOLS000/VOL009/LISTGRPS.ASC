3 REM PROGRAM TO LIST TRACK AND SECTOR ASSIGNMENTS FOR EACH
4 REM GROUP ON STANDARD CPM INTERLEAVE DISK
5 REM ALL VALUES ARE DECIMAL
6 REM TO PRINT ON LINE PRINTER CHANGE LINES 90 & 150 TO LPRINT
8 REM WRITTEN BY M.NICHOLS
9 REM
10 DIM D(104)
20 FOR I=1 TO 104
30 READ D(I)
35 IF I MOD 26 = 0 THEN RESTORE 260
40 NEXT I
50 G=0
60 T=2
70 B=1
80 C=B+7
90 PRINT "GROUP";G
100 FOR I=B TO C
110 IF I<=104 THEN A=3
120 IF I<=78 THEN A=2
130 IF I<=52 THEN A=1
140 IF I<=26 THEN A=0
150 PRINT " ","TRACK";T+A;"SECTOR";D(I)
160 NEXT I
170 IF C=104 THEN GOTO 220
180 B=B+8
190 C=C+8
200 G=G+1
210 GOTO 90
220 T=T+4
230 IF T>76 THEN GOTO 300
240 G=G+1
250 GOTO 70
260 DATA 1,7,13,19,25,5,11,17,23,3,9,15,21
265 DATA 2,8,14,20,26,6,12,18,24,4,10,16,22
300 END
