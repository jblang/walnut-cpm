
 TOP
 BYTE XX
 BYTE CR
 BYTE LF
 INT IAA
 INT IAB
 INT IBB
 INT ICC
 INT IDD
 INT IEE
 BEGINMAIN(AC,IAV)
 CR=+013
 LF=+010
	MS 'BEGIN T03'
 GOSUB CR
 IAA=+10001
 IBB=+10002
 XX=IAA==IBB
 IF XX
	MS '==FAILS!='
	MS 'OR IF  OR'
	MS 'DEFINITIN'
 ENDIF
 MS '111111111'
 IBB=+10001
 XX=IAA==IBB
 IF XX
 ELSE
	MS '==FAILED*'
 ENDIF
 MS '222222222'
 XX=IAA!=IBB
 IF XX
	MS '!=FAILED*'
 ENDIF
 MS '333333333'
 IBB=+10005
 XX=IAA!=IBB
 IF XX
 ELSE
	MS '!=FAILED*'
 ENDIF
 MS '444444444'
 IBB=+10001
 XX=IAA<!IBB
 IF XX
	MS '<!FAILS=='
 ENDIF
 MS '555555555'
 IBB=+10002
 XX=IBB<!IAA
 IF XX
	MS '<!FAILS>!'
 ENDIF
 MS '666666666'
 XX=IAA<!IBB
 IF XX
 ELSE
	MS '<!FAILS<!'
 ENDIF
 GOSUB CR
 MS '777777777'
 XX=IBB<=IAA
 IF XX
	MS '<=FAILS>!'
 ENDIF
 MS '888888888'
 IBB=+10001
 XX=IAA<=IBB
 IF XX
 ELSE
	MS '<=FAILS=='
 ENDIF
 MS '999999999'
 IBB=+10002
 XX=IAA<=IBB
 IF XX
 ELSE
	MS '<=FAILS<!'
 ENDIF
 MS '000000000'
 IAA++
 XX=IAA!=IBB
 IF XX
	MS '++FAILS..'
 ENDIF
 GOSUB CR
 MS 'AAAAAAAAA'
 IAA++
 IAA--
 XX=IAA!=IBB
 IF XX
	MS '--FAILS..'
 ENDIF
 MS 'BBBBBBBBB'
 IAA=+10050
 IBB=+10064
 IDD=IAA+IBB
 IEE=+20114
 XX=IDD!=IEE
 IF XX
	MS 'ADS...+++'
 ENDIF
 MS 'CCCCCCCCC'
 ICC=+10000
 IBB=+10050
 IDD=IBB-IAA
 IAA=+00000
 XX=IAA!=IDD
 IF XX
	MS 'SUB...+-+'
 ENDIF
 MS 'DDDDDDDDD'
 IAA=+00000
 IBB=+10050
 ICC=IAA-IBB
 IBB=+10064
 IDD=IAA-IBB
 IBB=+20114
 IEE=IAA-IBB
 IBB=ICC+IDD
 XX=IBB!=IEE
 IF XX
	MS 'ADS...-+-'
 ENDIF
 MS 'EEEEEEEEE'
 IBB=ICC-IDD
 IEE=+00014
 XX=IEE!=IBB
 IF XX
	MS 'SUB---=>+'
 ENDIF
 MS 'FFFFFFFFF'
 IBB=IDD-ICC
 IAA=+00000
 ICC=IAA-IEE
 XX=IBB!=ICC
 IF XX
	MS 'SUB---=>-'
 ENDIF
 MS 'GGGGGGGGG'
 IAA=+10001
 IBB=+10003
 IAA=IAA+IBB
 ICC=+20004
 XX=ICC!=IAA
 IF XX
	MS 'ADS.X=X+.'
 ENDIF
 MS 'HHHHHHHHH'
 IBB=IAA+IBB
 ICC=+30007
 XX=ICC!=IBB
 IF XX
	MS 'ADS.X=.+X'
 ENDIF
 GOSUB CR
 MS 'IIIIIIIII'
 ICC=ICC+ICC
 IBB=+60014
 XX=IBB!=ICC
 IF XX
	MS 'ADS.X=X+X'
 ENDIF
 MS 'JJJJJJJJJ'
 IAA=+10009
 IBB=+10005
 IAA=IAA-IBB
 ICC=+00004
 XX=ICC!=IAA
 IF XX
	MS 'SUB.X=X-.'
 ENDIF
 MS 'KKKKKKKKK'
 IAA=+10009
 IBB=IAA-IBB
 XX=ICC!=IBB
 IF XX
	MS 'SUB.X=.-X'
 ENDIF
 MS 'LLLLLLLLL'
 IAA=IAA-IAA
 IBB=+00000
 XX=IAA!=IBB
 IF XX
	MS 'SUB.X=X-X'
 ENDIF
 GOSUB CR
 	MS 'ENDOF T03'
 ENDMAIN
			SUB CR
 WRITE CR
 WRITE LF
 ENDSUB; CR
 BOTTOM
 FINIS
